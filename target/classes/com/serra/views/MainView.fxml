<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.web.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="vBox" alignment="TOP_CENTER" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.serra.controllers.MainController">

   <children>
      <StackPane fx:id="stackPane" VBox.vgrow="ALWAYS">
         <children>
            <ImageView fx:id="imageView" preserveRatio="true" />
            <Button fx:id="pressButton" text="ENTRAR A LA CALCULADORA" />
         </children>
      </StackPane>

      <Label fx:id="console" maxHeight="30" minHeight="30" text="Label">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin></Label>
   </children>
</VBox>
